{
  "name": "areum---clothing-rental",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:landing": "vite --mode landing",
    "dev:store": "vite --mode store",
    "build": "vite build",
    "build:landing": "vite build --mode landing && node scripts/rename-html.mjs landing",
    "build:store": "vite build --mode store && node scripts/rename-html.mjs store",
    "build:all": "npm run build:landing && npm run build:store",
    "preview": "vite preview",
    "preview:landing": "vite preview --outDir dist-landing",
    "preview:store": "vite preview --outDir dist-store --port 3001"
  },
  "dependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "lucide-react": "^0.562.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
